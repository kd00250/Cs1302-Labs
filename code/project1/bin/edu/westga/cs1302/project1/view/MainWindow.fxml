<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="458.0" minWidth="600.0" prefHeight="642.0" prefWidth="651.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.westga.cs1302.project1.view.MainWindow">
   <children>
      <Label layoutX="168.0" layoutY="100.0" text="Name" />
      <TextField fx:id="name" layoutX="109.0" layoutY="127.0" promptText="Enter Food Name" />
      <Label layoutX="171.0" layoutY="188.0" text="Type" />
      <ComboBox fx:id="type" layoutX="109.0" layoutY="215.0" prefWidth="150.0" />
      <Button layoutX="109.0" layoutY="272.0" mnemonicParsing="false" onAction="#addFood" text="Add Food" />
      <ListView fx:id="food" layoutX="226.0" layoutY="310.0" prefHeight="200.0" prefWidth="200.0" />
      <Label layoutX="383.0" layoutY="100.0" text="Quantity" />
      <TextField fx:id="quantity" layoutX="323.0" layoutY="127.0" prefHeight="25.0" prefWidth="166.0" promptText="Enter Quantity" />
      <Button layoutX="365.0" layoutY="163.0" mnemonicParsing="false" onAction="#setQuantity" text="Set Quantity" />
      <Button layoutX="499.0" layoutY="127.0" mnemonicParsing="false" onAction="#incrementQuantity" text="+" />
      <Button layoutX="499.0" layoutY="163.0" mnemonicParsing="false" onAction="#decrementQuantity" prefHeight="25.0" prefWidth="26.0" text="-" />
      <Button layoutX="184.0" layoutY="272.0" mnemonicParsing="false" onAction="#removeSelectedFood" text="Remove Food" />
   </children>
</AnchorPane>
