<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="379.0" minWidth="549.0" prefHeight="379.0" prefWidth="549.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.westga.cs1302.project1.view.MainWindow">
   <children>
      <Label layoutX="84.0" layoutY="21.0" text="Name" />
      <TextField fx:id="name" layoutX="26.0" layoutY="49.0" promptText="Enter Food Name" />
      <Label layoutX="251.0" layoutY="21.0" text="Type" />
      <ComboBox fx:id="type" layoutX="189.0" layoutY="49.0" prefWidth="150.0" />
      <Button layoutX="155.0" layoutY="320.0" mnemonicParsing="false" onAction="#addFood" text="Add Food" />
      <ListView fx:id="food" layoutX="177.0" layoutY="114.0" prefHeight="200.0" prefWidth="200.0" />
      <Label layoutX="419.0" layoutY="21.0" text="Quantity" />
      <TextField fx:id="quantity" layoutX="359.0" layoutY="49.0" prefHeight="25.0" prefWidth="166.0" promptText="Enter Quantity" />
      <Button layoutX="366.0" layoutY="80.0" mnemonicParsing="false" onAction="#setQuantity" text="Set Quantity" />
      <Button layoutX="484.0" layoutY="80.0" mnemonicParsing="false" onAction="#incrementQuantity" text="+" />
      <Button layoutX="452.0" layoutY="80.0" mnemonicParsing="false" onAction="#decrementQuantity" prefHeight="25.0" prefWidth="26.0" text="-" />
      <Button layoutX="230.0" layoutY="320.0" mnemonicParsing="false" onAction="#removeSelectedFood" text="Remove Food" />
      <Button layoutX="326.0" layoutY="320.0" mnemonicParsing="false" onAction="#countTotalItems" text="Count Items" />
   </children>
</AnchorPane>
